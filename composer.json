{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/AbdurRehman26/laravel-abstract-resource"
        }
    ],
    "require": {
        "php": "^8.2",
        "doctrine/dbal": "^2.8",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^7.8",
        "intervention/image": "^2.4",
        "kazmi/laravel-abstract-resource": "dev-master",
        "laravel-notification-channels/onesignal": "^2.4.0",
        "laravel/cashier": "^13.4",
        "laravel/framework": "^9.0",
        "laravel/passport": "^10.1.3",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.6",
        "nunomaduro/collision": "^6.1",
        "pda/pheanstalk": "^3.1",
        "predis/predis": "^1.1",
        "symfony/console": "^6.0",
        "yadakhov/insert-on-duplicate-key": "^1.2"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/sail": "^1.25",
        "mockery/mockery": "^1.0",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.4",
        "pestphp/pest-plugin-mock": "^1.0",
        "pestphp/pest-plugin-parallel": "^0.3.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Kazmi\\Resource\\": "packages/resource-commands/src"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
